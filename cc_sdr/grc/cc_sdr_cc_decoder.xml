<block>
  <name>CC Decoder</name>
  <key>cc_sdr_cc_decoder</key>
  <category>CC_SDR</category>
  <import>import cc_sdr</import>
  <make>cc_sdr.cc_decoder($has_fec, $has_rs, $has_white, $plen)</make>
  <param>
    <name>Viterbi FEC</name>
    <key>has_fec</key>
    <type>bool</type>
     <option>
       <name>Yes</name>
       <key>True</key>
     </option>
     <option>
       <name>No</name>
       <key>False</key>
     </option>
  </param>
  <param>
    <name>Reed Solomon</name>
    <key>has_rs</key>
    <type>bool</type>
     <option>
       <name>Yes</name>
       <key>True</key>
     </option>
     <option>
       <name>No</name>
       <key>False</key>
     </option>
  </param>
  <param>
    <name>Whitening</name>
    <key>has_white</key>
    <type>bool</type>
     <option>
       <name>Yes</name>
       <key>True</key>
     </option>
     <option>
       <name>No</name>
       <key>False</key>
     </option>
  </param>
  <param>
    <name>Plen</name>
    <key>plen</key>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
